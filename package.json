{
  "name": "monorepo-openapi",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.5.0",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
