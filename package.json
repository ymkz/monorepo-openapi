  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@types/node": "20.14.11",
    "concurrently": "8.2.2",
    "poku": "2.0.0",
    "turbo": "2.0.7",
    "typescript": "5.5.3"
  }
}
